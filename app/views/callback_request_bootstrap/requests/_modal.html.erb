
<div id="modCallback" class="modal fade"  tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog" >
    <div class="modal-content" >
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">Обратный звонок</h4>
      </div>
      <div class="modal-body">
        <div class="divContFormCallback"></div>
      </div>
      <% if 0==1 %>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary" name="submit">Submit</button>&nbsp;&nbsp;
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
      </div>
      <% end %>
    </div>
  </div>
</div>
